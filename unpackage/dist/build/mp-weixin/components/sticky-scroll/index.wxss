.sticky-scroll{box-sizing:border-box;width:100vw;height:100vh;overflow-y:auto;position:relative}.sticky-scroll .sticky-scroll-main{box-sizing:border-box}.sticky-scroll .sticky-scroll-main .sticky-scroll-navigation-bar{width:100vw;position:fixed;top:0;right:0;left:0;-webkit-backdrop-filter:blur(10px);backdrop-filter:blur(10px);z-index:5}.sticky-scroll .sticky-scroll-main .sticky-scroll-header{display:flex;align-items:center;column-gap:32rpx;position:-webkit-sticky;position:sticky;z-index:50;padding:0 32rpx;box-sizing:border-box}.sticky-scroll .sticky-scroll-main .sticky-scroll-header .title{font-family:Douyin Sans,Douyin Sans;font-weight:700;transition:.09s;font-size:56rpx;color:#333;line-height:66rpx}.sticky-scroll .sticky-scroll-main .sticky-scroll-header .back{width:68rpx;height:68rpx;background:rgba(255,255,255,.7);box-shadow:0 2rpx 23rpx rgba(158,158,158,.25);border-radius:50%;display:flex;justify-content:center;align-items:center}.sticky-scroll .sticky-scroll-main .sticky-scroll-header .back .back-icon{width:50rpx;height:50rpx;flex-shrink:0}
