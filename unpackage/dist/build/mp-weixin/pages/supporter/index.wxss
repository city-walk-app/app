.supporter{width:100vw;height:100vh;overflow-y:auto;box-sizing:border-box;display:flex;flex-direction:column;background-image:linear-gradient(to bottom,#ffd6c8 0%,#f9f5ee 100%);padding:90px 40px 170px}.supporter .header{padding-left:5px;box-sizing:border-box}.supporter .header .title{font-size:25px;color:#d8931c;display:flex;align-items:center}.supporter .header .title .city-icon{font-size:30px;color:#d8931c;margin-right:6px}.supporter .header .equitys{margin-top:16px;padding-left:3px;box-sizing:border-box}.supporter .header .equitys .equity-item{font-size:16px;line-height:28px;display:flex;align-items:center}.supporter .header .equitys .equity-item:before{content:"";display:block;width:3.6px;height:3.6px;background:red;border-radius:50%;margin-right:9px}.supporter .body{width:100%}.supporter .body .package-list{margin-top:40px}.supporter .body .package-list .package-item{width:100%;height:66px;display:flex;align-items:center;justify-content:space-between;padding:0 18px;box-sizing:border-box;margin-bottom:8px;background:rgba(255,255,255,.5);-webkit-backdrop-filter:blur(45px);backdrop-filter:blur(45px);box-shadow:rgba(0,0,0,.03) 0 2px 6px;border:2px solid transparent;transition:.26s}.supporter .body .package-list .package-item:nth-child(1){border-radius:20px 20px 20px 3px}.supporter .body .package-list .package-item:nth-child(2),.supporter .body .package-list .package-item:nth-child(3),.supporter .body .package-list .package-item:nth-child(4){border-radius:3px 20px 20px 3px}.supporter .body .package-list .package-item .package-item-title{font-size:16px;color:#111}.supporter .body .package-list .package-item .package-item-price{font-size:15px;font-weight:700;color:#d8931c}.supporter .body .package-list .package-item.package-item-active{border:2px solid #d8931c}.supporter .body .package-list .package-item.package-item-not{opacity:.6}
